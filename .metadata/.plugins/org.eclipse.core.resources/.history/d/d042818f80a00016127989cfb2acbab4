package io.theorem.albums.service;

import java.util.List;

import io.theorem.albums.entity.Album;
import io.theorem.albums.entity.Song;

public interface SongService {

	List<Song> findAllSongs();

	Song update(String songId, Song mv);

	void delete(String songId);

	Album createAlbum(Song mv);
	Album findById(String songId);
	
	
	
}
