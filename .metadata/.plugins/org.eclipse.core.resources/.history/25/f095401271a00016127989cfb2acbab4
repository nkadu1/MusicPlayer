package io.theorem.albums.exception;

@ResponseStatus(code=HttpStatus.BAD_REQUEST)

public class AlbumAlreadyExists extends RuntimeException{
	private static final long serialVersionUID = 1L;
	public AlbumAlreadyExistsException(String message) {
		super(message);
	}
	public AlbumAlreadyExistsException(String message, Throwable cause) {
		super(message,cause);
	}
	
}
